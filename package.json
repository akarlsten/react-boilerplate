    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.0",
    "babel-polyfill": "^6.26.0",
    "css-loader": "^0.28.9",
    "express": "^4.16.3",
    "extract-text-webpack-plugin": "^3.0.2",
    "firebase": "^5.9.2",
    "history": "^4.9.0",
    "moment": "^2.20.1",
    "node-sass": "^4.7.2",
    "normalize.css": "^7.0.0",
    "numeral": "^2.0.6",
    "raf": "^3.4.0",
    "react": "^16.2.0",
    "react-addons-shallow-compare": "15.6.0",
    "react-dates": "12.7.0",
